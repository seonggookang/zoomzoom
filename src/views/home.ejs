<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="https://unpkg.com/mvp.css" />
  </head>
  <body>
    <header>
      <h1>Zoom</h1>
    </header>
    <main>
      <div id="welcome">
        <form>
          <input placeholder="room name" required type="text" />
          <button>Enter room</button>
        </form>
      </div>

      <div id="call">
        <video
          id="myFace"
          autoplay
          playsinline
          width="400"
          height="400"
        ></video>
        <button id="mute">Mute</button>
        <button id="camera">Camera Off</button>
        <video
          id="peerFace"
          autoplay
          playsinline
          width="400"
          height="400"
        ></video>
      </div>
      <script src="/socket.io/socket.io.js"></script>
      <script src="/public/js/appzoom.js"></script>
      <script type="module" src="src/serverSocketIO.js"></script>
    </main>
  </body>
</html>
